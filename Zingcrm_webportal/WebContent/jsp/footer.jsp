<body>
	<script type="text/javascript">
		function popupdialog(url) {
			var newwindow;
			newwindow = window
					.open(
							url,
							'name',
							'height=500,width=400,left=800,top=100,resizable=yes,scrollbars=yes,toolbar=yes,status=yes');
			if (window.focus) {
				newwindow.focus();
			}
		}
	</script>
